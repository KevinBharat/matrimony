<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="my.matrimony">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Design.DashboardActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Design.RegistrationActivity" />
        <activity
            android:name=".Design.DisplayActivity"
            android:windowSoftInputMode="stateAlwaysHidden" />
        <activity android:name=".Design.DetailActivity" />
        <activity android:name=".Design.SearchActivity" />
        <activity android:name=".Design.CountryListActivity" />
        <activity android:name=".Design.StateListActivity"></activity>
    </application>

</manifest>